<?xml version="1.0"?>
<package>
  <name>jsk_mbzirc_common</name>
  <version>0.0.0</version>
  <description>The jsk_mbzirc_common package</description>

  <maintainer email="xychen@jsk.imi.i.u-tokyo.ac.jp">xychen at jsk</maintainer>
  <maintainer email="chou@jsk.imi.i.u-tokyo.ac.jp">zhao  at jsk</maintainer>

  <license>BSD</license>

  <url type="website">http://www.jsk.t.u-tokyo.ac.jp/</url>
